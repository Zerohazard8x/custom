@import url("https://fonts.googleapis.com/css?family=Inter|Roboto|Noto+Sans+JP|Noto+Sans+TC|Noto+Sans+SC|Noto+Sans+KR|Fira+Code|IBM+Plex+Mono|Libre+Baskerville");

:root {
    /* Define variables */
    --not-monospace: -apple-system, BlinkMacSystemFont, "SF Pro", "SF Pro Text",
        "Inter", "Roboto", "IBM Plex Sans", "Noto Sans", "Noto Sans JP",
        "Noto Sans TC", "Noto Sans SC", "Noto Sans KR", "IBM Plex Sans JP",
        "IBM Plex Sans KR";
	--serif: "Libre Baskerville", "EB Garamond";
    --monospace: "Fira Code", "Fira Mono", "Noto Mono", "Noto Sans Mono",
        "DroidSansM", "SF Mono", "SFMono-Regular", "CodeNewRoman Nerd Font",
        "Consolas", "Hack", "Hack Nerd Font", "Bitstream Vera Sans Mono",
        "BitstromWera Nerd Font", "DejaVu Sans Mono", "Cascadia Code",
        "CaskaydiaCove Nerd Font", "Cascadia Mono", "Segoe UI Mono",
        "Intel One Mono", Menlo, "MesloLG Nerd Font", Monaco, "Ubuntu Mono",
        "Ubuntu Mono Nerd Font", "Hasklig", "Hasklug Nerd Font",
        "Source Code Pro", "Source Code Pro Nerd Font", "Andale Mono",
        "Lucida Console", "Lucida Sans Typewriter";
    --block-mono: "IBM Plex Mono", "BlexMono Nerd Font", "Roboto Mono",
        "Fantasque Sans Mono", "FantasqueSansM Nerd Font", "Agave Nerd Font",
        "JetBrains Mono", "JetBrains Mono NL", "JetBrainsMono Nerd Font",
        ProFont, "Input Mono", "Envy Code R", "EnvyCodeR Nerd Font",
        "Iosevka Expanded", "Iosevka", "Iosevka Nerd Font",
        "IosevkaTerm Nerd Font", "Input Sans", "Liberation Mono";
    --mono-combo: var(--block-mono), var(--monospace);
}

@-moz-document domain("github.com") {
    /* Github - Change all fonts, change font and formatting of code blocks, grayscale */
    .Layout,
    .markdown-body,
    .page-responsive,
    .PageHeader-title,
    .Box-sc-g0xbh4-0,
    .Dialog__Footer-sc-uaxjsn-6,
    .my-3,
    .d-block,
    .container,
    section,
    details,
    blockquote,
    .container-lg {
        font-family: var(--not-monospace), -apple-system, BlinkMacSystemFont,
            "Segoe UI", "Noto Sans", Helvetica, Arial, sans-serif,
            "Apple Color Emoji", "Segoe UI Emoji" !important;
    }

    tt,
    #commit-summary-input,
    .blob-code-inner,
    .blob-num,
    .branch-name,
    .diffstat,
    .file-info,
    .sha,
    .sha-block,
    .text-mono,
    .input-monospace,
    .BranchName-sc-sg8jsy-0,
    pre,
    kbd,
    code {
        font-family: var(--mono-combo), ui-monospace, SFMono-Regular, SF Mono,
            Menlo, Consolas, Liberation Mono, monospace !important;

        /* Ligatures */
        font-variant-ligatures: common-ligatures, contextual !important;
        font-feature-settings: "liga" 1, "calt" 1 !important;

        /* -webkit-font-feature-settings: "liga", "calt" !important;
        -moz-font-feature-settings: "liga", "calt" !important;
        -o-font-feature-settings: "liga", "calt" !important; */
    }

    .blob-code-inner,
    .blob-num,
    pre {
        font-size: 16px !important;
    }

    div.logged-in.env-production.page-responsive.full-width,
    div.logged-in.env-production.page-responsive,
    div.sr-only {
        filter: grayscale(80%);
    }
}

