@-moz-document regexp(".*") {
    @import url("https://fonts.googleapis.com/css?family=Roboto+Flex|Fira+Mono|Source+Serif+4");
    @import url("https://fonts.googleapis.com/css?family=Source+Serif+4");
    /* @import url("https://fonts.googleapis.com/css?family=Fira+Mono"); */
    @import url("https://fonts.googleapis.com/css?family=Noto+Sans+JP|Noto+Sans+TC|Noto+Sans+SC|Noto+Sans+KR|Noto+Sans+Thai|Shippori+Mincho|Noto+Serif+JP|Noto+Serif+TC|Noto+Serif+SC|Noto+Serif+KR|Noto+Serif+Thai");
}