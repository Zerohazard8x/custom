==/UserStyle== */

@-moz-document regexp(".*") {
    @import url("https://fonts.googleapis.com/css2?family=Roboto+Flex|Fira+Mono:wght@100..900&display=swap");
    @import url("https://fonts.googleapis.com/css2?family=Source+Serif+4:wght@100..900&display=swap");
    /* @import url("https://fonts.googleapis.com/css2?family=Fira+Mono:wght@100..900&display=swap"); */
    @import url("https://fonts.googleapis.com/css2?family=Noto+Sans+JP|Noto+Sans+TC|Noto+Sans+SC|Noto+Sans+KR|Noto+Sans+Thai|Shippori+Mincho|Noto+Serif+JP|Noto+Serif+TC|Noto+Serif+SC|Noto+Serif+KR|Noto+Serif+Thai:wght@100..900&display=swap");
}